<%- model_class = ActsAsTaggableOn::Tag -%>
<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:name) %></th>
    </tr>
  </thead>
  <tbody>
    <% collection.each do |tag| %>
    <% tag=TagsDecorator.decorate(tag)%>
      <tr>
        <td><%= link_to tag.name,params.merge(:tag=>tag.name)  %></td>
      </tr>
    <% end %>
  </tbody>
</table>