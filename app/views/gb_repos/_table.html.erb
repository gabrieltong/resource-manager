<%- model_class = GbRepo -%>
<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:name) %></th>
      <th><%= model_class.human_attribute_name(:tags) %></th>
    </tr>
  </thead>
  <tbody>
    <% collection.each do |gb_repo| %>
      <tr>
        <td>
        	<%= link_to gb_repo.extra['name'], gb_repo_path(gb_repo) %>
        	<%= link_to :github,gb_repo.address,:class=>'btn btn-mini' %><br/>
        </td>
        <td>
          <%= render 'tags/input',:taggable=>gb_repo,:owner=>@user%>
      </tr>
    <% end %>
  </tbody>
</table>